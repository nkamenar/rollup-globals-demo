{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"jest/globals": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"plugin:jest/recommended"
	],
	"globals": {},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"strict": 2,
		"indent": ["off", "tab"],
		"semi": ["warn", "always"],
		"no-multi-spaces": "error"
	}
}
